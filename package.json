{
  "name": "siyuan-plugin-custom-slug",
  "version": "0.0.2",
  "type": "module",
  "description": "I heard give me a new name, it will be easier for you to find me",
  "repository": "terwer/siyuan-plugin-custom-slug",
  "homepage": "https://github.com/terwer/siyuan-plugin-custom-slug",
  "author": "terwer",
  "license": "GPL-3.0",
  "scripts": {
    "makeLink": "node  --no-warnings ./scripts/make_dev_link.js",
    "makeLog": "source scripts/venv/bin/activate && git-changelog -c angular --sections feat,fix,refactor,chore,docs -t angular -T -o CHANGELOG.md",
    "dev": "vite build --watch",
    "build": "vite build",
    "syncVersion": "python scripts/version.py",
    "parseChangelog": "python scripts/parse_changelog.py",
    "prepareRelease": "pnpm syncVersion && pnpm makeLog && pnpm parseChangelog"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@terwer/eslint-config-custom": "^1.2.0",
    "@tsconfig/svelte": "^4.0.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.2.0",
    "fast-glob": "^3.2.12",
    "glob": "^7.2.3",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "siyuan": "^0.7.1",
    "stylus": "^0.59.0",
    "svelte": "^3.57.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.7",
    "vite-plugin-static-copy": "^0.15.0",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "shorthash2": "^1.0.3",
    "transliteration": "^2.3.5",
    "zhi-lib-base": "^0.0.2"
  }
}
